{
  "name": "finalproject",
  "version": "1.0.0",
  "description": "CodeCore Final Project with React Redux Express & MongoDB ",
  "main": "index.js",
  "engines": {
    "node": "8.9.4",
    "npm": "5.7.1"
  },
  "scripts": {
